{
  "name": "@offbyonestudios/yarl",
  "version": "1.0.6",
  "description": "Yet Another React Library",
  "main": "index.js",
  "scripts": {
    "dev-client": "webpack-dev-server --port 3020 --env.target=web --env.yarlentry=true",
    "build-client": "webpack --env.target=web --env.yarlentry=true",
    "dev-server": "babel --watch src --out-dir dist",
    "cli": "node scripts/cli.js",
    "compile": "babel src --out-dir dist && babel scripts/dcli.js --out-file dist/cli.js",
    "prepublish": "npm run compile && mv dist/* ./",
    "postpublish": "git clean -df"
  },
  "bin": {
    "yarl": "dist/cli.js"
  },
  "author": "OffByOne Studios",
  "license": "MIT",
  "repository": "https://github.com/OffByOneStudios/yarl",
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "body-parser": "^1.17.1",
    "casual-browserify": "^1.5.11",
    "commander": "^2.9.0",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "graphql": "^0.9.2",
    "graphql-server-express": "^0.6.0",
    "graphql-tools": "^0.11.0",
    "react": "^15.4.2",
    "react-apollo": "^1.0.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
