{
  "name": "@offbyonestudios/yarl",
  "version": "1.0.3",
  "description": "Yet Another React Library",
  "main": "dist/index.js",
  "scripts": {
    "dev-client": "webpack-dev-server --port 3020 --env.yarltarget=browser --env.yarlentry=true",
